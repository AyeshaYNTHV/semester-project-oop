<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.ReportFormController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <TextField fx:id="reportTitleField" promptText="Enter report title"/>
    <TextArea fx:id="reportDescriptionArea" promptText="Enter report description"/>
    <Button fx:id="submitReportButton" text="Submit Report"/>
</VBox>
